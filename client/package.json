		"material-ui": "^0.16.4",
		"qs": "^6.3.0",
		"react": "^15.4.1",
		"react-css-modules": "^4.0.3",
		"react-document-title": "^2.0.2",
		"react-dom": "^15.4.1",
		"react-redux": "^5.0.1",
		"react-router": "^3.0.0",
		"react-router-redux": "^4.0.7",
		"react-tap-event-plugin": "^2.0.1",
		"redux": "^3.6.0",
		"redux-auth": "0.0.5-beta5",
		"redux-form": "^6.2.0",
